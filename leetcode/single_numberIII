class Solution {
public:
    vector<int> singleNumber(vector<int>& nums) {
         int xor_all = 0;
        for (int val : nums) {
            xor_all ^= val;
        }
        
        int rightmost_set_bit = xor_all & -xor_all;
        
        int num1 = 0, num2 = 0;
        for (int val : nums) {
            if (val & rightmost_set_bit) {
                num1 ^= val;
            } else {
                num2 ^= val;
            }
        }
        
        return {num1, num2};
    }

};